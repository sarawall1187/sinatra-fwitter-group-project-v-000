<h2><%= @tweet.content %> </h2>

<form  action="/tweets/<%= :slug %>" method="post">
 <input id="hidden" type="hidden" name="_method" value="patch">
 <input type="submit" name="submit" value="Edit Tweet">
</form>

<form  action="/tweets/<%= :slug %>/delete" method="post">
 <input id="hidden" type="hidden" name="_method" value="patch">
 <input type="submit" name="submit" value="Delete Tweet">
</form>
